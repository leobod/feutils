{
  "name": "@leobod/sot",
  "version": "0.0.9",
  "description": "",
  "main": "dist/sot.umd.js",
  "private": false,
  "files": [
    "/dist"
  ],
  "scripts": {
    "rm:all": "rimraf dist",
    "build:all": "npm run rm:all && npm run build:ts:commonjs && npm run build:ts:es6 && npm run build:webpack:umd &&  npm run build:webpack:commonjs2",
    "rm:ts": "rimraf dist/commonjs && rimraf dist/es6",
    "build:ts:commonjs": "tsc --project tsconfig.json",
    "build:ts:es6": "tsc --project tsconfig.es6.json",
    "rm:webpack": "rimraf dist/umd && rimraf dist/commonjs2",
    "build:webpack:umd": "webpack",
    "build:webpack:commonjs2": "webpack --config webpack.commonjs2.config.js",
    "rm:pkg": "rimraf dist/win64 && rimraf dist/mac64",
    "pkg:pre-win64": "pkg -t node10-win-x64 ./dist/commonjs/pkg/index.js -o dist/win64/sot.win64.exe",
    "pkg:pre-mac64": "pkg -t node10-macos-x64 ./dist/commonjs/pkg/index.js -o dist/mac64/sot.mac64.run",
    "pkg:win64": "npm run build:ts:commonjs && npm run pkg:pre-win64",
    "pkg:mac64": "npm run build:ts:commonjs && npm run pkg:pre-mac64",
    "pkg:all": "npm run build:ts:commonjs && npm run pkg:pre-win64 && npm run pkg:pre-mac64",
    "test:dev": "ts-node test/index.test.ts",
    "build:watch": "nodemon -e ts --exec ts-node src/out/umd.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/leobod/sot.git"
  },
  "keywords": [
    "util"
  ],
  "author": "leobod (leobod@eside.cn)",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/leobod/sot/issues"
  },
  "homepage": "https://github.com/leobod/sot#readme",
  "dependencies": {
    "axios": "^0.27.2",
    "commander": "^10.0.0",
    "core-js": "^3.6.5",
    "dayjs": "^1.10.7"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/preset-env": "^7.18.10",
    "@types/node": "^16.11.10",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "babel-eslint": "~10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.5",
    "eslint": "^7.1.0",
    "eslint-plugin-import": "^2.20.1",
    "nodemon": "^2.0.15",
    "pkg": "^5.8.0",
    "rimraf": "^4.1.2",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.8.2",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  }
}
