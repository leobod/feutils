{
  "name": "@esidecn/feutils",
  "version": "0.0.1",
  "description": "",
  "main": "./dist/umd/SmartTool.umd.js",
  "exports": {
    ".": {
      "import": "./dist/cjs/index.js",
      "require": "./dist/commonjs2/SmartTool.commonjs2.js",
      "default": "./dist/umd/SmartTool.umd.js"
    }
  },
  "bin": {},
  "private": false,
  "files": [
    "/dist"
  ],
  "scripts": {
    "rm:all": "rimraf dist",
    "rm:ts": "rimraf dist/lib",
    "build:ts": "tsc --project tsconfig.json",
    "build:webpack": "webpack --config webpack.config.js",
    "rm:pkg": "rimraf dist/win64 && rimraf dist/mac64",
    "pkg:pre-win64": "pkg -t node10-win-x64 ./dist/commonjs/pkg/index.js -o dist/win64/sot.win64.exe",
    "pkg:pre-mac64": "pkg -t node10-macos-x64 ./dist/commonjs/pkg/index.js -o dist/mac64/sot.mac64.run",
    "pkg:win64": "npm run build:ts && npm run pkg:pre-win64",
    "pkg:mac64": "npm run build:ts && npm run pkg:pre-mac64",
    "test:dev": "jest",
    "build:watch": "nodemon -e ts --exec ts-node src/out/umd.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/leobod/sot.git"
  },
  "keywords": [
    "util"
  ],
  "author": "leobod (leobod@eside.cn)",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/leobod/sot/issues"
  },
  "homepage": "https://github.com/leobod/sot#readme",
  "dependencies": {
    "axios": "^0.27.2",
    "commander": "^10.0.0",
    "core-js": "^3.6.5",
    "dayjs": "^1.10.7",
    "iconv": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@jest/globals": "^29.5.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^16.11.10",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "babel-eslint": "~10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.5",
    "eslint": "^7.1.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^29.5.0",
    "nodemon": "^2.0.15",
    "pkg": "^5.8.0",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2",
    "rollup": "^3.18.0",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.8.2",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
